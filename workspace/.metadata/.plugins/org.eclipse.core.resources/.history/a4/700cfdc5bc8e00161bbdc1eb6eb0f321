package com.k99.test;

public class Rectangle extends Graphical {
	private int a;//±ß³¤
	
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	
	public double computeArea() {
		return Math.pow(a, 2);
	}
	public double computePerimeter() {
		return 0.0;
	}
}
